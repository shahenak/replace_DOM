{
  "name": "replace_DOM",
  "description": "Replace DOM with image",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_title": "replace DOM with image",
    "default_icon": "blank.png"
  },

  "content_scripts": [{
      "matches": ["<all_urls>"],
      "js":      ["jquery-3.1.0.min.js", "content.js"]
  }],

  "web_accessible_resources": [
      "unable.png"
    ],

  "manifest_version": 2
}
